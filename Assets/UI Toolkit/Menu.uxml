        <ui:Label text="unnamed gdc officer game&#10;:DDD" display-tooltip-when-elided="true" enable-rich-text="false" style="font-size: 80px; -unity-text-align: upper-center;" />
        <ui:VisualElement name="PlayExpander" class="menu_button play_menu" style="overflow: hidden; width: 300px;">
            <ui:VisualElement name="PlayMenu" style="width: 100%; height: 160px; flex-direction: column; position: absolute; justify-content: space-between;">
                <ui:Button text="PLAY" display-tooltip-when-elided="true" name="PlayButton" enable-rich-text="false" class="bg_dark menu_button rounded text_light" style="font-size: 24px; margin-top: 0; margin-bottom: 0;" />
