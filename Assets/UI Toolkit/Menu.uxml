            </ui:VisualElement>
        </ui:VisualElement>
        <ui:Button text="OPTIONS" display-tooltip-when-elided="true" name="OptionsButton" enable-rich-text="false" class="bg_dark menu_button rounded text_light" style="width: 300px;" />
